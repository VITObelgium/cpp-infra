get_filename_component(GDX_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include(CMakeFindDependencyMacro)

list(APPEND CMAKE_MODULE_PATH ${GDX_CMAKE_DIR})

find_dependency(infra CONFIG REQUIRED)

list(REMOVE_AT CMAKE_MODULE_PATH -1)

if(NOT TARGET Gdx::core)
    include("${GDX_CMAKE_DIR}/GdxTargets.cmake")
endif()

set(Infra_LIBRARIES Infra::Infra)
